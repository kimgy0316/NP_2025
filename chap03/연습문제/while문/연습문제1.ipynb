   "source": [
    "n = int(input('Number: '))\n",
    "result = ''\n",
    "\n",
    "while n != 0:\n",
    "    m = n % 2\n",
    "    result = str(m) + result\n",
    "    n = n // 2 #몫\n",
    "\n",
    "print(\"2진수=\", result)\n"
   ]
  }
